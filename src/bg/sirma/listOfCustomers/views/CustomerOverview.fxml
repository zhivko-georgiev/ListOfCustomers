<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0"
	stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="bg.sirma.listOfCustomers.views.CustomerOverviewController">
	<children>
		<SplitPane dividerPositions="0.65" layoutX="153.0" layoutY="70.0"
			prefHeight="768.0" prefWidth="1024.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<items>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0"
					prefWidth="300.0">
					<children>
						<TableView fx:id="customerTable" layoutX="-12.0"
							layoutY="49.0" prefHeight="562.0" prefWidth="415.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
							<columns>
								<TableColumn fx:id="nameColumn" prefWidth="75.0"
									text="Име" />
								<TableColumn fx:id="townColumn" prefWidth="75.0"
									text="Населено Място" />
								<TableColumn fx:id="contractSignDateColumn"
									prefWidth="75.0" text="Дата на подписване на договор" />
								<TableColumn fx:id="notesColumn" prefWidth="75.0"
									text="Бележки" />
							</columns>
							<columnResizePolicy>
								<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
							</columnResizePolicy>
						</TableView>
					</children>
				</AnchorPane>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="562.0"
					prefWidth="348.0" styleClass="background">
					<children>
						<Label layoutX="104.0" layoutY="51.0" styleClass="label-header"
							text="Person Details" AnchorPane.leftAnchor="5.0"
							AnchorPane.topAnchor="5.0" />
						<GridPane layoutX="43.0" layoutY="87.0" prefHeight="173.0"
							prefWidth="393.0" AnchorPane.leftAnchor="5.0"
							AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="87.0">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="100.0" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<Label text="Име" />
								<Label text="Населено Място" GridPane.rowIndex="1" />
								<Label text="Дата на подписване на договор"
									GridPane.rowIndex="2" />
								<Label text="Бележки" GridPane.rowIndex="3" />
								<Label text="Договор" GridPane.rowIndex="4" />
								<Label fx:id="nameLabel" styleClass="label-bright" text="Label"
									GridPane.columnIndex="1" />
								<Label fx:id="townLabel" styleClass="label-bright" text="Label"
									GridPane.columnIndex="1" GridPane.rowIndex="1" />
								<Label fx:id="contractSignDateLabel" styleClass="label-bright"
									text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
								<Label fx:id="notesLabel" styleClass="label-bright" text="Label"
									GridPane.columnIndex="1" GridPane.rowIndex="3" />
								<Label fx:id="contractLabel" styleClass="label-bright"
									text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
							</children>
						</GridPane>
						<ButtonBar layoutX="54.0" layoutY="250.0"
							AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
							<buttons>
								<Button mnemonicParsing="false" onAction="#handleNewCustomer"
									text="New..." />
								<Button mnemonicParsing="false" onAction="#handleEditCustomer"
									text="Edit..." />
								<Button minWidth="66.0" mnemonicParsing="false"
									onAction="#handleDeleteCustomer" prefHeight="33.0" prefWidth="120.0"
									text="Delete" />
							</buttons>
						</ButtonBar>
						<AnchorPane layoutX="88.0" layoutY="281.0" maxHeight="200.0"
							maxWidth="200.0" minHeight="200.0" minWidth="200.0" prefHeight="200.0"
							prefWidth="200.0">
							<children>
								<ImageView fx:id="logoImage" fitHeight="200.0"
									fitWidth="200.0" />
							</children>
						</AnchorPane>
					</children>
				</AnchorPane>
			</items>
		</SplitPane>
	</children>
</AnchorPane>
